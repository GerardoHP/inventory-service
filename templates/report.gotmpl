<!doctype html>
<head>
</head>
<body>
    <h1>Product Summary Report</h1>

    <table>
        <thead>
            <tr>
                <th>Row</th>
                <th>Product Name</th>
                <th>Quantity On Hand</th>
            </tr>
        </thead>
        <tbody>
            {{range $index, $element := . }}
            <tr>
                <td>{{$index +1}}</td>
                <td>{{.ProductName}}</td>
                <td>{{.QuantityOnHand}}</td>
            </tr>
            {{end}}
        </tbody>
    </table>
</body>
